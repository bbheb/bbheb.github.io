					<key>source</key>
					<string>const frontmost_app_name = Application('System Events').applicationProcesses.where({ frontmost: true }).name()[0];
const app = Application(frontmost_app_name);
const jScript = "var x = document.querySelectorAll('.pull-left label'),i;for (i = 0; i &lt; x.length; i++) { x[i].style.fontFamily = 'Times New Roman'; x[i].style.fontSize = '36px';}"

if (frontmost_app_name == "Safari"){
	app.doJavaScript(jScript, {in: app.windows[0].currentTab()});
}else{ 
	try{
		app.windows[0].activeTab.execute({ javascript: jScript});
	} catch(e){
		app.displayNotification('Hello', { withTitle: 'Error' });
	}
}</string>
